page {
    width: 100%;
    height: 100%;
    background-color: #f5f6f7;
}

.dati-top {
    color: #fff;
    padding-top: 45rpx;
    padding-bottom: 30rpx;
    text-align: center;
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
}

.circle-box {
    position: relative;
    height: 520rpx;
    width: 100%;
}

.circle {
    position: absolute;
    left: 0;
    right: 0;
    top: 50px;
    margin: auto;
    z-index: 999999;
}

.draw_btn {
    width: 35vw;
    position: absolute;
    top: 170rpx;
    right: 0;
    left: 0;
    margin: auto;
    border-radius: 5vw;
    color: #fff;
    text-align: center;
}

.draw_top {
    top: 193rpx;
}

.draw_btn .title {
    font-size: 46rpx;
}

.draw_btn .ok {
    font-size: 23rpx;
}

.bg {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
}

.down {
    position: absolute;
    left: 0;
    right: 0;
    top: 334rpx;
    margin: auto;
    text-align: center;
    font-size: 25rpx;
    color: #fff;
}

.orange {
    color: #fed531;
}

.subject text {
    font-size: 36rpx;
    font-weight: 500;
}

.all {
    margin-top: 5px;
}

.errorgo {
    padding-left: 30rpx;
    padding-right: 30rpx;
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #d9d9d9;
    align-items: center;
    background: #fff;
}

.wenb {
    color: #7f7f7f;
    font-size: 23rpx;
}

.erroe-left {
    line-height: 45rpx;
}

.erroe-left text {
    color: #fa3638;
}

.erroe-right {
    width: 140rpx;
    height: 50rpx;
    background: #1bd0ad;
    color: #fff;
    text-align: center;
    line-height: 50rpx;
    border-radius: 5px;
    font-size: 23rpx;
}

.share {
    width: 444rpx;
    height: 82rpx;
    border-radius: 40rpx;
    background: #fff;
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    bottom: -42rpx;
    box-shadow: 0px 2px 8px #fee1d6;
    color: #1bd0ad;
    line-height: 82rpx;
}

.result_banner {
    width: 600px;
    height: 480px;
    position: fixed;
    left: -1000px;
    top: -1000px;
}

.result_share {
    width: 750px;
    height: 1030px;
    position: fixed;
    left: -1000px;
    top: -1000px;
}

ad {
    position: fixed;
    bottom: 0;
    left: 0;
}

.intentify_tag {
    border: 1px solid #ff7022;
    color: #ff7022;
    padding-left: 3px;
    padding-right: 3px;
    border-radius: 10px;
    font-size: 23rpx;
    line-height: 25rpx;
}